
<td id="snsMenu">
<div id="snsMenuContainer">
<div class="profileContainer">
<%= render :partial => "sns/admin/base/project_menu"%>
</div>
</td>
<td  id="snsContent" colspan="2" class="project">
<div class="menu">
  <div class="leftMenu configs">
    <h2 class="project">プロジェクト編集</h2>
<%
is_sys_admin=false
is_sys_admin = true if Core.user.has_auth?(:manager)
%>
  <%= form_for :item, :url => {:action => :update}, :html => {:method => :put} do |f| %>

  <%= f.error_messages %>
  <%if is_sys_admin%>
  <%= render :partial => "form", :locals => {:f => f, :submitclass=>:edit} %>
  <%else%>
  <%= render :partial => "manager_form", :locals => {:f => f, :submitclass=>:edit} %>
  <%end%>
<% end %>
  </div>
</div>
</td>
