<div id="snsMenuContainer">
<div class="profileContainer">
  <%= render :partial => "sns/admin/base/profile_menu" %>
</div>
</div>
</td>

<td id="snsContent" colspan="2">

<div class="bread_crumbs">

</div>

<div class="menu">
  <div class="leftMenu">
    <%if @items.blank?%>
      <span>更新履歴が存在しません。</span>
    <%else%>
    <table class="index">
      <tr>
        <th>変更点</th>
        <th>変更したユーザー</th>
      </tr>
      <%for item in @items%>
       <tr>
          <td><%=raw item.text%></td>
         <td><%=item.created_user.name if item.created_user%></td>
       </tr>
      <%end%>
    </table>
    <%end%>
  </div>
</div>
