<div class="photo">
<%
photo_items = []
group_link = ""
group_class = ""
other_group_link = ""
photo_items = Sns::Photo.any_in("_id"=>photo_ids)
photo_items.each_with_index{ |c, i|
link_str = ""
link_str = image_tag(c.resized_public_uri, c.display_options)
if feed_user.blank?
link_uri = ""
else
link_uri = "#{sns_user_photo_path(feed_user.account,c,{:album_id=>c.album_id})}"
end
group_link = c.public_uri if i==0
%>
<%= link_to(link_str, c.public_uri,:class=>"photoGroup group#{item.id}", :id=>"popUpPhoto_#{c.id}_#{item.id}",:target=>"_blank", :rel=>"group#{item.id}")%>
<%} if photo_items%>
<br />
</div>