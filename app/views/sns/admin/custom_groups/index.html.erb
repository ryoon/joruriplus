
<td id="snsMenu">
<div id="snsMenuContainer">
<div class="leftMenu">
    <%= render :partial => "sns/admin/profile_configs/editing_menu", :locals => {:current => @edit_current }%>
</div>
</div>
</td>

<td id="snsContent" colspan="2" class="<%=@edit_current%>">

<div class="bread_crumbs">
</div>
<div class="menu">
  <div class="leftMenu configs">
  <h2 class="preview">プレビュー</h2>
  <h3 class="titleLine">
    <span>カスタムグループ設定</span>
  <%=link_to("グループを新規作成する",new_sns_custom_group_path, :class=>"create buttonNew") unless @friends.blank?%>
  </h3>

  <% if flash[:notice] %><div class="notice"><%= flash[:notice] %></div><% end %>
  <%=link_to("友達一覧へ戻る",sns_friends_path)%>
    <%if @items.blank?%>
    表示するカスタムグループが存在しません。
    <%else%>
    <table class="index">
      <tr>
        <th>グループ名称</th>
      </tr>
    <%for item in @items%>
    <tr>
      <td><%=link_to item.display_title, sns_custom_group_path(item)%></td>
    </tr>
    <%end%>
    </table>
    <%end%>
  </div>
</div>
</td>
