jQuery.noConflict();
jQuery(document).ready(function($){
<%unless @post.blank? && @dupulicate_check.blank?%>
var addList = '<%= escape_javascript(render :partial=>"ajax_create", :locals=>{:post=>@post, :kind=>@kind}) %>';
$("#like<%=@post.id%>").html(addList);
$("#likeBtn<%=@post.id%>").html('<%= escape_javascript(link_to "いいねを取り消す", delete_sns_like_path(@item,{:p_id=>@post.id,:kind=>@kind}),:remote => true) %>');
$("#likeBtn<%=@post.id%>").attr("class","censelLike");
<%end%>
});
