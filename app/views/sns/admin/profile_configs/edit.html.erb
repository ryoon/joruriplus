<%
if @edit_current == "addr"
 title_str = "連絡先"
elsif @edit_current == "skill"
 title_str = "特技"
elsif @edit_current == "interest"
 title_str = "趣味、関心"
elsif @edit_current == "pr"
 title_str = "PR"
else
 title_str = "基本情報"
end
%>

<td  id="snsMenu">
<div id="snsMenuContainer">
<div class="leftMenu">
    <%= render :partial => "sns/admin/profile_configs/editing_menu", :locals => {:current => @edit_current }%>
</div>

</div>
</td>
<td  id="snsContent" colspan="2">
<div class="menu">
  <div class="leftMenu configs">
  <h2><%=@item.name%></h2>
  <div class="titleLine"><%= title_str%></div>
  <%= form_for :item, :url => {:action => :update}, :html => {:method => :put} do |f| %>

  <%= f.error_messages %>
  <%= render :partial => "forms", :locals => {:f => f, :current=>@edit_current} %>


<% end %>
  </div>
</div>
</td >
